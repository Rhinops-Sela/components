{
  "datadog": {
    "apiKey": "",
    "appKey": "",
    "logs": {
      "enabled": true,
      "containerCollectAll": true
    },
    "clusterAgent": {
      "enabled": true,
      "metricsProvider": {
        "enabled": true
      },
      "rbac": {
        "create": true,
        "serviceAccountName": "default"
      },
      "tolerations": [
        {
          "key": "monitoring",
          "operator": "Equal",
          "value": "true"
        }
      ],
      "nodeSelector": {
        "role": "monitoring"
      }
    }
  }
}